{
    "configurations": [
        {
            "name": "STM32L4",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Core/Src",
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Tests/Src",
                "${workspaceFolder}/Tests/Inc",
                "${workspaceFolder}/Drivers/STM32L4xx_HAL_Driver/Inc"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "STM32L432xx",
                "USE_HAL_DRIVER",
                "__CC_ARM"
            ],
            "intelliSenseMode": "${default}",
            "compilerPath": "${env:ARMGCC_DIR}\\arm-none-eabi-gcc.exe",
            "compilerArgs": [
                "-mcpu-cortex-m4",
                "-mthumb"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}